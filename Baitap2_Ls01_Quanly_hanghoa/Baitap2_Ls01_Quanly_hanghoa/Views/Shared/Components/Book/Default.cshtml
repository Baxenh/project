@model IEnumerable<Baitap2_Ls01_Quanly_hanghoa.Controllers.Book>
<div class="row">

    @foreach (var item in Model)
    {
        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
            <div class="thumbnail">
                <img src="@item.Image" alt="">
                <div class="caption">
                    <h3>@item.Title</h3>
                    <p> price:@item.Price</p>
                </div>
            </div>
        </div>
    }


</div>